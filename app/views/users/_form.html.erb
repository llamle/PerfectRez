<form class="" action="<%= users_path %>" method="post">
  <div class="userAuth">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <input value="user" type="hidden" name="user[role]" id="user_role">
  </div>
  <div class="field">
    <label for="user_username">Username</label><br>
    <input type="text" class="materialize-textarea" name="user[username]" id="user_username">
  </div>
  <div class="field">
    <label for="user_first_name">First name</label><br>
    <input type="text" class="materialize-textarea" name="user[first_name]" id="user_first_name">
  </div>
  <div class="field">
    <label for="user_last_name">Last name</label><br>
    <input type="text" class="materialize-textarea" name="user[last_name]" id="user_last_name">
  </div>
  <div class="field">
    <label for="user_email">Email</label><br>
    <input type="email" class="materialize-textarea" name="user[email]" id="user_email">
  </div>
  <div class="field">
    <label for="user_password">Password</label><br>
    <input type="password" class="materialize-textarea" name="user[password]" id="user_password">
  </div>
  <div class="field">
    <label for="user_password_confirmation">Password Confirmation</label><br>
    <input type="password" class="materialize-textarea" name="user[password_confirmation]" id="user_password_confirmation">
  </div>
  <div class="field">
    <input type="hidden" class="materialize-textarea" name="user[session_id]" id="user_session_id" value="<%= session.id %>">
  </div>
  <div class="field">
    <label for="user_email_token">Email token</label><br>
    <input type="text" class="materialize-textarea" name="user[email_token]" id="user_email_token">
  </div>
  <div class="actions">
    <button class="btn waves-effect waves-light makeBlue" type="submit" name="commit" value="Create User">Submit
      <i class="fa fa-paper-plane"></i>
    </button>
  </div>
</form>
